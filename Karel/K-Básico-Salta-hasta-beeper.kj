class program {
    void saltar(){
         turnleft();
         move();
         turnleft();
         turnleft();
         turnleft();
         move();
         turnleft();
         turnleft();
         turnleft();
         move();
         turnleft();
    }
    
    program () {
        while(notNextToABeeper()){
            if(frontIsBlocked()){
               saltar();
            }else{
               move();
            }
        }
        turnoff();
    }
}
